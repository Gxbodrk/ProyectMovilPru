<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linearLayout6"
        android:gravity="center"
        android:padding="20px"
        android:layout_gravity="center">
        <TextView
            android:text="LA SANTA SED"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:textStyle="bold"
            android:textSize="50px"
            android:layout_marginBottom="15dp"
            android:textColor="#ff000000"
            android:layout_gravity="center" />

        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout1"
            android:background="@drawable/borde"
            android:layout_marginBottom="10dp">
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView1"
                android:background="@drawable/cag_cerv1" />
            <Button
                android:text="Cervezas"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:id="@+id/button1"
                android:background="@android:color/transparent"
                android:textStyle="bold" />
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:background="@drawable/cag_cerv2" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout2"
            android:layout_marginBottom="10dp"
            android:background="@drawable/borde_c2">
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView3"
                android:background="@drawable/cag_lico1" />
            <Button
                android:text="Licores"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:id="@+id/button2"
                android:background="@android:color/transparent"
                android:textStyle="bold" />
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView4"
                android:background="@drawable/cag_lico2" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout3"
            android:layout_marginBottom="10dp"
            android:background="@drawable/borde_c3">
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView5"
                android:background="@drawable/cag_sh1" />
            <Button
                android:text="Sin Alcohol"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:id="@+id/button3"
                android:background="@android:color/transparent"
                android:textStyle="bold" />
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView6"
                android:background="@drawable/cag_sh2" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout4"
            android:background="@drawable/borde_c4"
            android:layout_marginBottom="10dp">
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView7"
                android:background="@drawable/cag_snk1" />
            <Button
                android:text="Snacks"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:id="@+id/button4"
                android:background="@android:color/transparent"
                android:textStyle="bold" />
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView8"
                android:background="@drawable/cag_snk2" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:id="@+id/linearLayout5"
            android:background="@drawable/borde_c5"
            android:gravity="center">
            <TextView
                android:layout_width="70.7dp"
                android:layout_height="match_parent"
                android:id="@+id/textView9"
                android:background="@drawable/cag_otro1" />
            <Button
                android:text="Otros"
                android:layout_width="170dp"
                android:layout_height="match_parent"
                android:id="@+id/button5"
                android:background="@android:color/transparent"
                android:textStyle="bold" />
            <TextView
                android:layout_width="87dp"
                android:layout_height="match_parent"
                android:id="@+id/textView10"
                android:background="@drawable/cag_otro2"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
