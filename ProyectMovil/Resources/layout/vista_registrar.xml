<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff215d71"
    android:minWidth="25px"
    android:minHeight="25px"
    android:paddingLeft="30dp"
    android:paddingRight="30dp"
    android:gravity="center">
    <TextView
        android:text="¡REGISTRATE!"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_marginTop="24.5dp"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="#ffffffff"
        android:textSize="80px"
        android:layout_marginBottom="21.7dp"/>
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="426.0dp"
        android:id="@+id/linearLayout1"
        android:gravity="center"
        android:background="@drawable/icon_registroo">
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="83.0dp"
            android:id="@+id/linearLayout2">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="150.0dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout3">
                <TextView
                    android:text="Nombre:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView11"
                    android:textStyle="bold"
                    android:textSize="45px"
                    android:textColor="#ffffffff" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@id/editText1"
                    android:textColor="#ffffffff" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout4"
                android:layout_marginLeft="4dp">
                <TextView
                    android:text="Apellido:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView12"
                    android:textStyle="bold"
                    android:textColor="#ffffffff"
                    android:textSize="45px" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@id/editText2"
                    android:textColor="#ffffffff" />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:text="Usuario o Apodo:"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView10"
            android:textColor="#ffffffff"
            android:textSize="45px"
            android:textStyle="bold" />
        <EditText
            android:inputType="textMultiLine"
            android:layout_width="match_parent"
            android:layout_height="55.0dp"
            android:id="@id/editText3"
            android:textColor="#ffffffff" />
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="83.0dp"
            android:id="@+id/linearLayout5"
            android:gravity="center">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="147.0dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout6">
                <TextView
                    android:text="Cédula:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView13"
                    android:textColor="#ffffffff"
                    android:textStyle="bold"
                    android:textSize="45px" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText4"
                    android:textColor="#ffffffff" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="147.0dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout7"
                android:layout_marginLeft="4dp">
                <TextView
                    android:text="Teléfono:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView14"
                    android:textStyle="bold"
                    android:textSize="45px"
                    android:textColor="#ffffffff" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText5"
                    android:textColor="#ffffffff" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="83.0dp"
            android:id="@+id/linearLayout8"
            android:gravity="center">
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="205dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout9"
                android:layout_marginRight="0.0dp">
                <TextView
                    android:text="Contraseña:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView15"
                    android:textColor="#ffffffff"
                    android:textStyle="bold"
                    android:textSize="45px" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText6"
                    android:textColor="#ffffffff" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:id="@+id/linearLayout10"
                android:layout_marginLeft="4dp">
                <TextView
                    android:text="Edad:"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView16"
                    android:textSize="45px"
                    android:textStyle="bold"
                    android:textColor="#ffffffff"
                    android:layout_marginLeft="2dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/editText7"
                    android:textColor="#ffffffff" />
            </LinearLayout>
        </LinearLayout>
        <TextView
            android:text="Correo:"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            android:textColor="#ffffffff"
            android:textStyle="bold"
            android:textSize="45px" />
        <EditText
            android:inputType="textMultiLine"
            android:layout_width="match_parent"
            android:layout_height="54.0dp"
            android:id="@+id/editText8"
            android:textColor="#ffffffff" />
    </LinearLayout>
    <Button
        android:text="Button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button3" />
    <LinearLayout
        android:orientation="horizontal"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/linearLayout11"
        android:gravity="center"
        android:layout_marginTop="16.3dp">
        <Button
            android:text="Iniciar Sesión"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:id="@+id/button1"
            android:layout_marginRight="10dp" />
        <Button
            android:text="Registrarse"
            android:layout_width="125dp"
            android:layout_height="match_parent"
            android:id="@+id/button2"
            android:layout_marginLeft="10dp" />
    </LinearLayout>

</LinearLayout>