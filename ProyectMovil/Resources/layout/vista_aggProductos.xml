<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff5c6982">
    <ImageButton
        android:src="?android:attr/panelBackground"
        android:layout_width="45.5dp"
        android:layout_height="46.0dp"
        android:id="@+id/imageButton1"
        android:layout_marginTop="7.5dp"
        android:layout_marginLeft="4.0dp"
        android:background="@mipmap/atras" />
    <LinearLayout
        android:orientation="vertical"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="476.5dp"
        android:id="@+id/linearLayout7"
        android:gravity="center">
        <TextView
            android:text="Agregar productos"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="35.5dp"
            android:minWidth="25px"
            android:textAlignment="center"
            android:minHeight="25px"
            android:id="@+id/textView1"
            android:layout_marginLeft="0.0dp"
            android:layout_marginTop="0.0dp"
            android:textColor="@android:color/background_light" />
        <LinearLayout
            android:orientation="vertical"
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="353.5dp"
            android:layout_height="378.0dp"
            android:id="@+id/linearLayout1"
            android:layout_marginTop="63.5dp"
            android:layout_marginBottom="0.0dp"
            android:background="@android:drawable/divider_horizontal_textfield"
            android:layout_marginLeft="0.0dp"
            android:layout_marginRight="0.0dp"
            android:gravity="center">
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="54.0dp"
                android:id="@+id/linearLayout2"
                android:gravity="right">
                <TextView
                    android:text="Categoria:"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="74.5dp"
                    android:layout_height="42.0dp"
                    android:id="@+id/textView2"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginTop="13.0dp"
                    android:textColor="@android:color/holo_orange_dark"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:layout_marginRight="21.0dp" />
                <Spinner
                    android:layout_width="104.0dp"
                    android:layout_height="40.5dp"
                    android:id="@+id/spinner1"
                    android:layout_marginTop="4.5dp"
                    android:layout_marginLeft="21.0dp"
                    android:layout_marginRight="24.5dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="74.5dp"
                android:id="@+id/linearLayout3"
                android:gravity="right">
                <TextView
                    android:text="Nombre del producto:"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="166.0dp"
                    android:layout_height="45.5dp"
                    android:id="@+id/textView3"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginTop="18.0dp"
                    android:textColor="@android:color/holo_orange_dark"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:layout_marginRight="34.5dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="114.5dp"
                    android:layout_height="46.0dp"
                    android:id="@+id/editText1"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="16.5dp"
                    android:layout_marginTop="14.5dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="54.5dp"
                android:id="@+id/linearLayout4"
                android:gravity="right">
                <TextView
                    android:text="Stock:"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="46.5dp"
                    android:layout_height="39.0dp"
                    android:id="@+id/textView4"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginTop="0.0dp"
                    android:textColor="@android:color/holo_orange_dark"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:layout_marginRight="56.0dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="92.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/editText2"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="21.5dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="54.5dp"
                android:id="@+id/linearLayout5"
                android:gravity="right">
                <TextView
                    android:text="Precio:"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/textView5"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginTop="15.5dp"
                    android:textColor="@android:color/holo_orange_dark"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:layout_marginRight="50.0dp" />
                <EditText
                    android:inputType="textMultiLine"
                    android:layout_width="95.5dp"
                    android:layout_height="match_parent"
                    android:id="@+id/editText3"
                    android:layout_marginLeft="0.0dp"
                    android:layout_marginRight="20.5dp" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="51.0dp"
                android:id="@+id/linearLayout6"
                android:layout_marginTop="57.0dp"
                android:gravity="center">
                <Button
                    android:text="Agregar Producto"
                    android:layout_width="319.0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/button1"
                    android:layout_marginLeft="0.0dp"
                    android:background="@android:color/holo_orange_dark"
                    android:gravity="center" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
